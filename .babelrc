{
	"env": {
		"cjs": {
			"presets": [
				["env", {
					"targets": {
						"browsers": [
							"IE >= 11",
							"last 2 Chrome versions",
							"last 2 Firefox versions",
							"Firefox ESR",
							"last 2 Edge versions",
							"last 2 Safari major versions",
							"last 2 iOS major versions"
						]
					}
				}]
			]
		},
		"es": {
			"presets": [
				["env", {
					"modules": false,
					"targets": {
						"browsers": [
							"IE >= 11",
							"last 2 Chrome versions",
							"last 2 Firefox versions",
							"Firefox ESR",
							"last 2 Edge versions",
							"last 2 Safari major versions",
							"last 2 iOS major versions"
						]
					}
				}]
			]
		},
		"test": {
			"presets": [
				["env", {
					"targets": {
						"node": "8"
					}
				}]
			]
		}
	}
}
